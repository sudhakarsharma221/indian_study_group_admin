    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/bottom_nav"
    app:startDestination="@id/navigation_library">
    <fragment
        android:id="@+id/navigation_library"
        android:name="com.indiastudygroupadmin.bottom_nav_bar.library.ui.LibraryFragment"
        android:label="Home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_schedule"
        android:name="com.indiastudygroupadmin.bottom_nav_bar.schedule.ui.ScheduleFragment"
        android:label="Schedule"
        tools:layout="@layout/fragment_schedule" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.indiastudygroupadmin.bottom_nav_bar.profile.ProfileFragment"
        android:label="Profile"
        tools:layout="@layout/fragment_profile" />

    <fragment
        android:id="@+id/navigation_more"
        android:name="com.indiastudygroupadmin.bottom_nav_bar.more.MoreFragment"
